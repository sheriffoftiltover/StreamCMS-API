{
    "name" : "streamcms/api",
    "description": "The API for Classes/StreamCMS",
    "license": "proprietary",
    "version": "1.0.0",
    "minimum-stability": "dev",
    "require": {
        "php" : ">=8.0",
        "ext-json": "*",
        "ext-xml": "*",
        "ext-mbstring": "*",
        "ext-intl": "*",
        "doctrine/dbal": "~2.13",
        "doctrine/orm": "~2.9",
        "doctrine/annotations": "~1.13",
        "doctrine/cache": "~1.11",
        "doctrine/persistence": "~2.2",
        "scienta/doctrine-json-functions": "~4.3",
        "beberlei/doctrineextensions": "~1.3",
        "symfony/process": "~5.3",
        "symfony/dotenv": "~5.3",
        "guzzlehttp/guzzle": "~7.3",
        "paypal/merchant-sdk-php" : "3.12.*",
        "monolog/monolog" : "1.x-dev",
        "ramsey/uuid": "~4.1",
        "league/route": "~4.5",
        "laminas/laminas-diactoros": "~2.6",
        "laminas/laminas-httphandlerrunner": "~1.4",
        "robmorgan/phinx": "~0.11",
        "nesbot/carbon": "~2.49",
        "symfony/console": "~4.4",
        "adhocore/jwt": "*",
        "defuse/php-encryption": "~2.3",
    },
    "require-dev": {
        "fakerphp/faker": "~1.14",
        "phpunit/phpunit": "~9.5",
        "symfony/framework-bundle": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "StreamCMS\\": "./Classes"
        }
    },
    "scripts": {
        "build": [
            "npm --prefix /home/StreamCMS run build"
        ]
    },
    "extra": {
        "preload": {
            "paths": [
                "Classes/StreamCMS"
            ],
            "exclude": [
                "vendor",
                ".composer_cache"
            ],
            "extensions": [
                "php",
                "module",
                "inc",
                "install"
            ],
            "exclude-regex": "/[A-Za-z0-9_]test\\.php$/i",
            "no-status-check": false
        }
    }
}
