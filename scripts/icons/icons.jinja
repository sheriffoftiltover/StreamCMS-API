/* glue: {{ version }} hash: {{ hash }} */

/* start destiny.gg */
.icon-subscriber,
.icon-subscribert2,
.icon-subscribert3,
.icon-subscribert4 {
    -webkit-transform: rotate(0deg);
    -webkit-transition: rotate 0.2s ease-in-out;
    -moz-transition: rotate 0.2s ease-in-out;
    -o-transition: rotate 0.2s ease-in-out;
    transition: rotate 0.2s ease-in-out;  
}
.icon-subscriber:hover,
.icon-subscribert2:hover,
.icon-subscribert3:hover,
.icon-subscribert4:hover {
    -moz-animation: spin 3s infinite linear;
    -webkit-animation: spin 3s infinite linear;
}
/* end destiny.gg */

{% for image in images %}.{{ image.label }}{{ image.pseudo }}{%- if not image.last %}, {%- endif %}{%- endfor %}{
    background-image:url('{{ sprite_path }}');
    background-repeat:no-repeat;
}
{% for image in images %}
.{{ image.label }}{{ image.pseudo }}{
    background-position:{{ image.x ~ ('px' if image.x) }} {{ image.y ~ ('px' if image.y) }};
    width:{{ image.width }}px;
    height:{{ image.height }}px;
}
{% endfor %}